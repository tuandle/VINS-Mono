<?xml version="1.0"?>
<launch>
    <arg name="config_path" default = "$(find feature_tracker)/../config/realsense/ironsides_factory_left_config.yaml" />
	  <arg name="vins_path" default = "$(find feature_tracker)/../config/../" />
    
    <node name="feature_tracker" pkg="feature_tracker" type="feature_tracker" output="log">
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>

    <node name="vins_estimator" pkg="vins_estimator" type="vins_estimator" output="screen">
       <param name="config_file" type="string" value="$(arg config_path)" />
       <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>

    <node name="pose_graph" pkg="pose_graph" type="pose_graph" output="screen">
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="visualization_shift_x" type="int" value="0" />
        <param name="visualization_shift_y" type="int" value="0" />
        <param name="skip_cnt" type="int" value="0" />
        <param name="skip_dis" type="double" value="0" />
    </node>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find vins_estimator)/../config/vins_rviz_config.rviz" output="screen" />
    
    <!-- <include file="$(find geodetic_utils)/launch/gps_to_pose_conversion.launch" />  
        <node pkg="hector_trajectory_server" type="hector_trajectory_server" name="hector_trajectory_server" output="screen">
            <param name="target_frame_name" type="string" value="map" />
            <param name="source_frame_name" type="string" value="" />
            <param name="trajectory_update_rate" type="double" value="4" />
            <param name="trajectory_publish_rate" type="double" value="0.25" />
        </node>
    -->

    
</launch>
